{
  "name": "test-svelte-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:newbornfrontender/test-svelte-template.git",
  "author": "Infant Frontender <newbornfrontender@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@commitlint/config-conventional": "8.0.0",
    "babel-eslint": "10.0.1",
    "commitlint": "8.0.0",
    "cross-env": "5.2.0",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.1.10",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-svelte3": "1.2.3",
    "husky": "2.4.0",
    "lint-staged": "8.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.17",
    "postcss-preset-env": "6.6.0",
    "prettier": "1.18.2",
    "prettier-plugin-svelte": "0.6.0",
    "rollup": "1.16.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-livereload": "1.0.0",
    "rollup-plugin-node-resolve": "5.0.1",
    "rollup-plugin-svelte": "5.0.3",
    "rollup-plugin-terser": "5.0.0",
    "sirv-cli": "0.4.3",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-standard": "18.3.0",
    "svelte": "3.4.4",
    "svelte-preprocess": "2.12.0"
  },
  "scripts": {
    "format": "prettier --write --svelte-sort-order scripts-markup-styles ./**/*.{svelte,html,css,js,json,md}",
    "lint:js": "eslint --fix ./src/**/*.{svelte,js}",
    "lint:js:check": "eslint --print-config . | eslint-config-prettier-check",
    "lint:css": "stylelint --fix ./**/*.{svelte,css}",
    "lint:css:check": "stylelint-config-prettier-check",
    "build": "cross-env NODE_ENV=production rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public",
    "start:dev": "sirv public --dev"
  }
}
